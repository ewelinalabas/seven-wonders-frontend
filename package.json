    "reselect": "^3.0.1",
    "rimraf": "^2.6.2",
    "tslib": "^1.9.0",
    "yargs": "^10.1.1"
  },
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@types/material-ui": "^0.21.1",
    "@types/query-string": "^5.0.1",
