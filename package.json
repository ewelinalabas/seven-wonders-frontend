    "path-to-regexp": "^2.2.0",
    "query-string": "^5.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.3.1",
    "react-loadable-visibility": "^2.4.2",
