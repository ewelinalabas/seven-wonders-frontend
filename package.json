    "lint": "tslint \"src/**/*.ts?(x)\" \"app/**/*.ts?(x)\" \"test/**/*.ts?(x)\" -t stylish",
    "lint:fix": "tslint \"src/**/*.ts?(x)\" \"app/**/*.ts?(x)\" \"test/**/*.ts?(x)\" -t stylish --fix",
    "git:hook": "chmod 755 .githook/pre-commit && cd .git/hooks && rm -f pre-commit && ln -s ../../.githook/pre-commit"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cross-env": "^5.1.1",
    "dotenv": "^5.0.1",
