  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "./.*\\.spec\\.(ts|tsx)$",
    "collectCoverageFrom": [
      "src/**/*"
    ],
    "coveragePathIgnorePatterns": [
      "/src/.*\\.d\\.ts$"
    ],
    "unmockedModulePathPatterns": [
      "react"
    ]
  }
