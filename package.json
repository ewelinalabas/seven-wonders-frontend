    "css-loader": "^0.28.8",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "html-webpack-plugin": "^3.0.6",
    "jest": "^22.3.0",
    "jest-junit": "^3.6.0",
    "koa-connect": "^2.0.1",
    "prettier": "^1.9.2",
    "style-loader": "^0.19.1",
    "ts-jest": "^22.0.4",
    "ts-loader": "^4.0.1",
    "tslint": "^5.8.0",
    "tslint-react": "^3.2.0",
